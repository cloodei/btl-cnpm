{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/create/page.tsx",
    "app/page.tsx",
    "app/layout.tsx",
    "app/login/layout.jsx",
    "app/login/page.tsx"
, "components/navbar.jsx", "app/my-decks/page.tsx", "components/auth/login-form.jsx", "components/mode-toggle.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
